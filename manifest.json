{
    "manifest_version": 2,
   
    "name": "ADO Outlook Extension",
    "version": "0.1.0",
    "description": "View Outlook Emails in an ADO Work Item",
   
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["https://microsoft.visualstudio.com/OS/_workitems/edit/*"],
      "run_at" : "document_idle"
    }]
   
  }